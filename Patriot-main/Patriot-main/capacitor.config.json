{
  "appId": "com.arcticfoxgames.patriot",
  "appName": "Patriot",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "buildOptions": {
      "keystorePath": "",
      "keystoreAlias": ""
    }
  },
  "ios": {
    "contentInset": "never",
    "limitsNavigationsToAppBoundDomains": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#000000",
      "showSpinner": false
    },
    "Keyboard": {
      "resize": "none"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#000000"
    }
  },
  "cordova": {
    "preferences": {
      "Orientation": "landscape",
      "Fullscreen": "true",
      "StatusBarOverlaysWebView": "false",
      "StatusBarBackgroundColor": "#000000"
    }
  }
}
